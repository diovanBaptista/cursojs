<!DOCTYPE html>
<html lang="pt-br">
<%- include('./partial/head.ejs') %>
<body>
  <%- include('./partial/navbar.ejs') %>
    <div class="content">
      <% if (users.length > 0) { %> <% users.forEach(user => { %>
        <h3 class="title">
          <%= user.firstName %> <%= user.lastName %>
        </h3>
        <p><%= user.email %></p>
        <% }) %> <% } else { %>
          <p>Não ha Usuários para exibir...</p>
          <% } %>
    </div>

</body>
</html>